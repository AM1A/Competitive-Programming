#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() 
{
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n,i;
    scanf("%d\n",&n);
    int arr[n],arn[100] = {0};
    
    for(i = 0;i<n;i++)
    {
        scanf("%d ",&arr[i]);
        arn[arr[i]]++;
    }
    
    for(i = 0;i<100;i++)
    {
        printf("%d ",arn[i]);
    }
    return 0;
}
